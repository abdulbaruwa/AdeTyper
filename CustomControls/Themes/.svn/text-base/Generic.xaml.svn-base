<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CustomControls">


    <!--
    Encapsulate actual control definitions in separate files for easy maintenance
    * Note requirement to provide FULL PATH (not just file name) here, inside Generic.xaml
    -->

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/CustomControls;component/Themes/resCustomWindow.xaml" />
        <ResourceDictionary Source="/CustomControls;component/Themes/resXSButton.xaml" />
    </ResourceDictionary.MergedDictionaries>
    
    
</ResourceDictionary>
